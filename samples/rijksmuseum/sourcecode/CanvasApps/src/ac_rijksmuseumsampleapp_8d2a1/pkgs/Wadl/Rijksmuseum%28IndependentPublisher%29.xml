  <resources siena:authenticationProviderHref="#PowerAppAuth" base="https://europe-002.azure-apim.net/apim/rijksmuseumip">
    <resource path="/{connectionId}/api/{culture}/collection/">
      <param style="template" name="connectionId" type="xs:string" required="true" />
      <param style="template" name="culture" type="xs:string" required="true" default="nl">
        <doc title="The language to search in (and of the results)." />
        <option value="nl" />
        <option value="en" />
      </param>
      <method siena:requiresAuthentication="true" name="GET" id="GetCollection" actionName="GetCollection" siena:isDeprecated="false">
        <doc title="Search the Collection&#xA;This operation is used for searching the rijksmuseum  collection" />
        <request>
          <param style="query" name="objectnumber" type="xs:string">
            <doc title="The identifier of the object (case-sensitive)." />
          </param>
          <param style="query" name="involvedMaker" type="xs:string">
            <doc title="Object needs to be made by this agent." />
          </param>
          <param style="query" name="p" type="xs:long" default="0">
            <doc title="The result page. Note that p * ps cannot exceed 10,000." />
          </param>
          <param style="query" name="ps" type="xs:long" default="10">
            <doc title="The number of results per page. Note that p * ps cannot exceed 10,000. ps can have a value from 1-100." />
          </param>
          <param style="query" name="s" type="xs:string">
            <doc title="Sorting of the Results" />
            <option value="relevance" />
            <option value="objecttype" />
            <option value="chronologic" />
            <option value="achronologic" />
            <option value="artist" />
            <option value="artistdesc" />
          </param>
          <param style="query" name="toppieces" type="xs:boolean" default="false">
            <doc title="Only give works that are top pieces." />
            <option value="true" />
            <option value="false" />
          </param>
          <param style="query" name="imgoly" type="xs:boolean" default="false">
            <doc title="Only give results for which an image is available or not." />
            <option value="true" />
            <option value="false" />
          </param>
          <param style="query" name="f.dating.period" type="xs:long">
            <doc title="The century in which the object is made (0-21)." />
          </param>
          <param style="query" name="title" type="xs:string">
            <doc title="Title of the Object" />
          </param>
          <param style="query" name="technique" type="xs:string">
            <doc title="The technique used to make the object." />
          </param>
          <param style="query" name="material" type="xs:string">
            <doc title="The material of the object." />
          </param>
          <param style="query" name="type" type="xs:string">
            <doc title="The type of the object." />
          </param>
          <param style="query" name="place" type="xs:string">
            <doc title="The place where the object was produced" />
          </param>
        </request>
        <response>
          <representation mediaType="application/json" element="service:GetCollection_responses_200_def" />
        </response>
      </method>
    </resource>
    <resource path="/{connectionId}/api/{culture}/usersets/{user-id}-{collection-name}">
      <param style="template" name="connectionId" type="xs:string" required="true" />
      <param style="template" name="culture" type="xs:string" required="true" default="nl">
        <doc title="The language to search in (and of the results)." />
        <option value="nl" />
        <option value="en" />
      </param>
      <param style="template" name="user-id" type="xs:long" required="true">
        <doc title="Specify an individual user-id" />
      </param>
      <param style="template" name="collection-name" type="xs:string" required="true">
        <doc title="Specify your collection name (in small letter and spaces to be replaced with &quot;-&quot;)" />
      </param>
      <method siena:requiresAuthentication="true" name="GET" id="GetUsersets" actionName="GetUsersets" siena:isDeprecated="false">
        <doc title="User-generated content&#xA;These APIs give access to sets of objects created by users in Rijksstudio." />
        <request>
          <param style="query" name="format" type="xs:string" default="json">
            <doc title="The format of the result." />
            <option value="json" />
            <option value="jsonp" />
            <option value="xml" />
          </param>
          <param style="query" name="page" type="xs:long" default="0">
            <doc title="The result page to fetch. Note that page * pageSize cannot exceed 10.000." />
          </param>
          <param style="query" name="pageSize" type="xs:long" default="25">
            <doc title="The number of results per page. Note that page * pageSize cannot exceed 10.000." />
          </param>
        </request>
        <response>
          <representation mediaType="application/json" element="service:GetUsersets_responses_200_def" />
        </response>
      </method>
    </resource>
